<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="../../../../favicon.ico">

    <title>{% block title %}D.U.K{% endblock %}</title>

    <!-- Bootstrap core CSS -->
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" rel="stylesheet">
    {% block stylesheets %}
        <link href="https://unpkg.com/jquery.splitter/css/jquery.splitter.css" rel="stylesheet">
        {% stylesheets '@AppBundle/Resources/Public/Styles.css' filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>



</head>

<body>
{% if is_granted('ROLE_USER') %}
<nav class="navbar navbar-expand-md navbar-dark bg-dark">
    <a class="navbar-brand" href="{{ path('faq_index') }}">Helper</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarsExampleDefault" aria-controls="navbarsExampleDefault" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarsExampleDefault">
        <ul class="navbar-nav">


            {% if is_granted('ROLE_MANAGER') %}
                <a class="nav-link" href="{{ path('faq_new') }}">Nauja kategorija</a><br>

                <a class="nav-link" href="{{ path('faq_new_question') }}">Naujas Klausimas </a>

            {% endif %}
        </ul>
        <ul class="nav navbar-nav ml-auto">
            {% if is_granted('ROLE_ADMIN') %}
                <li>
                    <a class="nav-link" href="{{ path('admin')}}">Admin </a>
                </li>
            {% endif %}
            <li>
                <a class="nav-link" href="{{ path('fos_user_security_logout') }}">Atsijungti </a>
            </li>
        </ul>

       {# {{ form(searchBar.searchBar) }}#}

    </div>
</nav>
{% endif %}
<main role="main" class="container-fluid" >
    <div class="row">
        <div class="col-md-12">
            {% block body %}{% endblock %}
        </div>
    </div>
</main><!-- /.container -->

<script src="http://code.jquery.com/jquery-3.1.0.min.js"></script>
<script src="https://unpkg.com/jquery.splitter/js/jquery.splitter.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>


{% block javascripts %}
{% endblock %}

</body>
</html>



